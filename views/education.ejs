
<% layout('boilerplate') -%>
<div class="container my-5">
    <h2 class="text-center mb-4">Education</h2>
    <div class="row">
      <!-- 10th Grade -->
      <div class="col-md-3 mb-4">
        <div class="card h-100 border-0 shadow-sm ">
         
            <img src="Punjab-Board.jpg" class="card-img-top" alt="Krishna Model High School Logo">
         
          <div class="card-body text-center">
            <h4 class="card-title  font-weight-bold">10th Grade</h4>
            <p class="card-text fs-6"><strong>School:</strong> Krishna Model High School</p>
            <p class="card-text fs-6"><strong>Board:</strong> PSEB (Punjab School Education Board)</p>
            <p class="card-text fs-6"><strong>Year:</strong> 2018</p>
            <p class="card-text fs-5 font-weight-bold"><strong>Percentage:</strong> 87%</p>
          </div>
        </div>
      </div>
      <!-- 12th Grade -->
      <div class="col-md-3 mb-4">
        <div class="card h-100 border-0 shadow-sm">
          <img src="Punjab-Board.jpg" class="card-img-top" alt="Ramgarhia Girls Senior Secondary School Logo">
          <div class="card-body text-center">
            <h4 class="card-title  font-weight-bold">12th Grade</h4>
            <p class="card-text fs-6"><strong>School:</strong> Ramgarhia Girls Senior Secondary School</p>
            <p class="card-text fs-6"><strong>Board:</strong> PSEB (Punjab School Education Board)</p>
            <p class="card-text fs-6"><strong>Year:</strong> 2020</p>
            <p class="card-text fs-5 font-weight-bold"><strong>Percentage:</strong> 89%</p>
          </div>
        </div>
      </div>
      <!-- Diploma -->
      <div class="col-md-3 mb-4">
        <div class="card h-100 border-0 shadow-sm">
          <img src="th.jpeg" class="card-img-top" alt="Government Girls Polytechnic Lucknow Logo">
          <div class="card-body text-center">
            <h4 class="card-title font-weight-bold">Diploma</h4>
            <p class="card-text fs-6"><strong>Institute:</strong> Government Girls Polytechnic Lucknow</p>
            <p class="card-text fs-6"><strong>Board:</strong> State Board of Technical Education Uttar Pradesh</p>
            <p class="card-text fs-6"><strong>Year:</strong> 2022</p>
            <p class="card-text fs-5 font-weight-bold"><strong>Percentage:</strong> 83%</p>
          </div>
        </div>
      </div>
      <!-- B.Tech -->
      <div class="col-md-3 mb-4">
        <div class="card h-100 border-0 shadow-sm">
          <img src="iet.jpeg" class="card-img-top" alt="IET Lucknow Logo">
          <div class="card-body text-center">
            <h4 class="card-title font-weight-bold">B.Tech</h4>
            <p class="card-text fs-6"><strong>College:</strong> IET Lucknow</p>
            <p class="card-text fs-6"><strong>University:</strong> Abdul Kalam Technical University</p>
            <p class="card-text fs-6"><strong>Year:</strong> 2025</p>
            <p class="card-text fs-5 font-weight-bold"><strong>CGPA:</strong> 8.18</p>
          </div>
        </div>
      </div>
    </div>
  </div>
